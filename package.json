{
  "name": "subfont",
  "version": "6.1.0",
  "description": "Speeds up your pages initial paint by automatically subsetting local or Google fonts and loading them optimally",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "lint": "eslint . && prettier --check '**/*.{js,md}'",
    "test": "mocha && npm run lint",
    "travis": "npm run coverage && npm run lint",
    "coverage": "nyc --reporter=lcov --reporter=text -- mocha",
    "preversion": "offline-github-changelog --next=${npm_package_version} > CHANGELOG.md && git add CHANGELOG.md"
  },
  "bin": {
    "subfont": "lib/cli.js"
  },
  "main": "lib/subfont.js",
  "files": [
    "lib",
    "*.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Munter/subfont.git"
  },
  "keywords": [
    "google",
    "font",
    "fonts",
    "webfont",
    "webfonts",
    "subset",
    "subsetting",
    "commandline",
    "cli",
    "automation",
    "woff",
    "woff2",
    "preload"
  ],
  "author": "Peter MÃ¼ller <munter@fumle.dk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Munter/subfont/issues"
  },
  "homepage": "https://github.com/Munter/subfont#readme",
  "dependencies": {
    "@gustavnikolaj/async-main-wrap": "^3.0.1",
    "assetgraph": "^7.0.1",
    "browserslist": "^4.16.6",
    "css-font-parser": "^0.3.0",
    "css-font-weight-names": "^0.2.1",
    "css-list-helpers": "^2.0.0",
    "font-family-papandreou": "^0.2.0-patch2",
    "font-snapper": "^1.2.0",
    "font-tracer": "^2.0.1",
    "fontkit": "^1.8.1",
    "fontverter": "^2.0.0",
    "gettemporaryfilepath": "^1.0.1",
    "lines-and-columns": "^1.1.6",
    "lodash": "^4.17.21",
    "memoizesync": "^1.1.1",
    "postcss": "^8.3.5",
    "postcss-value-parser": "^4.1.0",
    "pretty-bytes": "^5.6.0",
    "puppeteer-core": "^10.1.0",
    "specificity": "^0.4.1",
    "subset-font": "^1.3.3",
    "urltools": "^0.4.2",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "combos": "^0.2.0",
    "coveralls": "^3.1.1",
    "css-generators": "^0.4.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^4.1.0",
    "html-generators": "^1.0.3",
    "httpception": "^4.0.1",
    "magicpen-prism": "^5.0.0",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "offline-github-changelog": "^2.3.0",
    "prettier": "~2.3.2",
    "proxyquire": "^2.1.3",
    "puppeteer": "^10.1.0",
    "sinon": "^11.1.1",
    "unexpected": "^12.0.3",
    "unexpected-check": "^3.0.0",
    "unexpected-resemble": "^5.0.0",
    "unexpected-set": "^3.3.0",
    "unexpected-sinon": "^11.0.1"
  }
